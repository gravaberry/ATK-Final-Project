<h4 class="text-capitalize">Form Edit Jenis Barang</h4>

<div class="row">
    <div class="col-lg-6">
        <form action="/master/satuan/proses-update/<%= satuan[0].id %>" method="post">
            <div class="mb-3">
                <label class="form-label">Kode satuan</label>
                <input type="text" class="form-control" name="f_kode" value="<%= satuan[0].kode %>">
            </div>
            <div class="mb-3">
                <label class="form-label">Nama satuan</label>
                <input type="text" class="form-control" name="f_nama_satuan" value="<%= satuan[0].nama_satuan %>">
            </div>
            <div class="mb-3">
                <label class="form-label">Description</label>
                <input type="text" class="form-control" name="f_description" value="<%= satuan[0].description %>">
            </div>
            <button type="submit" class="btn btn-warning">Update</button>
        </form>
    </div>
</div>