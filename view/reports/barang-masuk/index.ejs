<button class=" btn btn btn-info">
    <h4 class="text-capitalize">Laporan Barang Stok Masuk</h4>
</button>

<div class="card">
    <div class="card-body">
        
    <div class="col-12 pt-3">
        <div class="row">
            <div class="col-lg-12">
                <table id="myTable" class="table table-hover">
                    <thead>
                        <tr>
                            <th>No</th>
                            <th>Nama Barang</th>
                            <th>Stok Masuk</th>
                            <th>satuan</th>
                            <th>Tanggal Masuk</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for( let i=0; i < stok_masuk.length; i++) { %>
                            <tr>
                                <td><%= i + 1 %></td>
                                <td><%= stok_masuk[i].nama_barang %></td>
                                <td><%= stok_masuk[i].stok_awal %></td>
                                <td><%= stok_masuk[i].nama_satuan %></td>
                                <td><%= stok_masuk[i].createad_at.toLocaleDateString('id-ID') %></td>
                               
                                </td>
                            </tr>
                        <% } %>
                    </tbody>
                    
                </table>
            </div>
        </div>
    </div>
    </div>
</div>